{
  "coc.preferences.formatOnSaveTimeout": 500,
  "coc.preferences.formatOnType": true,
  "prettier.tabWidth": 2,
  "refactor.saveToFile": true,
  "coc.preferences.formatOnSave": true,
  "flutter.provider.enableSnippet": true,
  "suggest.autoTrigger": "always",
  "diagnostic.virtualText": true,
  "diagnostic.checkCurrentLine": true,
  "flutter.runDevToolsAtStartup": true,
  "flutter.autoHideDevLog": true,
  "flutter.autoOpenDevLog": true,
  "python.autoComplete.addBrackets": true,
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "python.formatting.provider": "autopep8",
  "python.linting.pylintPath": "pylint",
  "python.linting.pylintArgs": ["--rcfile=.pylintrc"],
  "languageserver": {
    "dart": {
      "command": "dart",
      "args": [],
      "filetypes": ["dart"],
      "trace.server": "verbose"
    }
  },
  "tailwindCSS.enable": true,
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.classAttributes": ["class", "className", "ngClass", "tw"],
  "tailwindCSS.jsLanguages": [
    "javascript",
    "javascriptreact",
    "reason",
    "typescriptreact"
  ],
  "tailwindCSS.cssLanguages": [
    "css",
    "less",
    "postcss",
    "sass",
    "scss",
    "stylus",
    "vue"
  ],
  "tailwindCSS.htmlLanguages": [
    "blade",
    "edge",
    "eelixir",
    "ejs",
    "elixir",
    "elm",
    "erb",
    "eruby",
    "haml",
    "handlebars",
    "html",
    "htmldjango",
    "HTML (EEx)",
    "HTML (Eex)",
    "html.twig",
    "jade",
    "leaf",
    "markdown",
    "njk",
    "nunjucks",
    "php",
    "razor",
    "slim",
    "svelte",
    "twig",
    "vue"
  ],
  "tailwindCSS.suggestions": true,
  "tailwindCSS.headwind.runOnSave": true,
  "tailwindCSS.showPixelEquivalents": true,
  "tailwindCSS.colorDecorators": true,
  "codeLens.enable": true,
  "rust-analyzer.checkOnSave": true
}
