{
  "coc.preferences.formatOnSaveTimeout": 500,
  "snippets.userSnippetsDirectory": "~/.config/nvim/snippets/",
  "coc.preferences.formatOnType": true,
  "prettier.tabWidth": 2,
  "refactor.saveToFile": true,
  "coc.preferences.formatOnSave": true,
  "suggest.autoTrigger": "always",
  "diagnostic.virtualText": true,
  "diagnostic.checkCurrentLine": true,
  "flutter.runDevToolsAtStartup": true,
  "flutter.autoOpenDevLog": true,
  "languageserver": {
    "dart": {
      "command": "dart",
      "args": [],
      "filetypes": ["dart"],
      "trace.server": "verbose"
    },
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.work", "go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    },
    "kotlin": {
      "command": "~/dev/kotlin-lsp/server/bin/kotlin-language-server",
      "filetypes": ["kotlin"]
    }
  },
  "tailwindCSS.enable": true,
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.classAttributes": ["class", "className", "ngClass", "tw"],
  "tailwindCSS.jsLanguages": [
    "javascript",
    "javascriptreact",
    "reason",
    "typescriptreact"
  ],
  "tailwindCSS.cssLanguages": [
    "css",
    "less",
    "postcss",
    "sass",
    "scss",
    "stylus",
    "vue"
  ],
  "tailwindCSS.htmlLanguages": [
    "blade",
    "edge",
    "eelixir",
    "ejs",
    "elixir",
    "elm",
    "erb",
    "eruby",
    "haml",
    "handlebars",
    "html",
    "htmldjango",
    "HTML (EEx)",
    "HTML (Eex)",
    "html.twig",
    "jade",
    "leaf",
    "markdown",
    "njk",
    "nunjucks",
    "php",
    "razor",
    "slim",
    "svelte",
    "twig",
    "vue"
  ],
  "tailwindCSS.suggestions": true,
  "tailwindCSS.headwind.runOnSave": true,
  "tailwindCSS.showPixelEquivalents": true,
  "tailwindCSS.colorDecorators": true,
  "codeLens.enable": true,
  "rust-analyzer.checkOnSave": true,
  "snippets.ultisnips.pythonPrompt": false,
  "coc.preferences.extensionUpdateCheck": "daily"
}
